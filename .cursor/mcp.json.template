{
  "mcpServers": {
    "github": {
      "command": "C:\\Users\\{USERNAME}\\AppData\\Local\\pypoetry\\Cache\\virtualenvs\\{VENV_NAME}\\Scripts\\github-mcp-server.exe",
      "args": ["stdio", "--toolsets=all"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "your_github_token_here"
      }
    },
    "supabase": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--access-token",
        "your_supabase_token_here"
      ]
    },
    "context7": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    }
  }
} 